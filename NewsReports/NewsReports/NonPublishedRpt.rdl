  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>NewsDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserID">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SectionID">
            <Value>=Parameters!SectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     COUNT(*) AS NewsNumber, AdminUsers.UserID, AdminUsers.FullName, MainSections.SectionID, MainSections.SecTitle
FROM         News INNER JOIN
                      AdminUsers ON News.Creator = AdminUsers.UserID INNER JOIN
                      MainSections ON News.SectionID = MainSections.SectionID
WHERE     (News.NewStatus &lt;&gt; 7) AND (News.JournalID = 1) AND (News.PublishDate BETWEEN CONVERT(date, @DateFrom, 103) AND CONVERT(date, @DateTo, 103)) AND 
                      (AdminUsers.UserID = CASE WHEN @UserID = 0 THEN AdminUsers.UserID ELSE @UserID END) AND 
                      (MainSections.SectionID = CASE WHEN @SectionID = 0 THEN MainSections.SectionID ELSE @SectionID END)
GROUP BY AdminUsers.UserID, AdminUsers.FullName, MainSections.SectionID, MainSections.SecTitle
ORDER BY NewsNumber DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="NewsNumber">
          <DataField>NewsNumber</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UserID">
          <DataField>UserID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SectionID">
          <DataField>SectionID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SecTitle">
          <DataField>SecTitle</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
