  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>NewsDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserID">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        AdminUsers.FullName, AdminUsers.UserID, 

COUNT(CASE WHEN ( (News.sUploaderID = AdminUsers.UserID) and (News.NewStatus = 7) ) THEN 1 ELSE NULL END) AS 'PublishCount', 
COUNT(CASE WHEN ( (News.sDeskID = AdminUsers.UserID) and (News.NewStatus = 8) ) THEN 1 ELSE NULL END) AS 'RejectedCount', 
COUNT(CASE WHEN ( (News.sDeskID = AdminUsers.UserID) and (News.NewStatus = 9) ) THEN 1 ELSE NULL END) AS 'WaitingCount'


FROM            News LEFT OUTER JOIN
                         AdminUsers ON AdminUsers.UserID = News.sUploaderID 


WHERE    (News.JournalID = 1) 
AND (News.AddedDate BETWEEN CONVERT(date, @DateFrom, 103) AND CONVERT(date,@DateTo, 103))
 AND( (News.NewStatus = 7) OR (News.NewStatus = 8) OR (News.NewStatus = 9))AND 
 (AdminUsers.UserID = CASE WHEN @UserID= 0 THEN AdminUsers.UserID ELSE  @UserID END)
GROUP BY AdminUsers.FullName, AdminUsers.UserID</CommandText>
      </Query>
      <Fields>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserID">
          <DataField>UserID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PublishCount">
          <DataField>PublishCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
