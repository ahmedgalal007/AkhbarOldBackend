  <DataSources>
    <DataSource Name="NewsDS">
      <DataSourceReference>NewsDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4f4b07f9-f8db-4c95-939f-3986dd1987a7</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>NewsDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(CASE WHEN News.newstatus = 8 THEN 1 ELSE NULL END) AS 'RejectedNewsCount', COUNT(CASE WHEN News.newstatus = 9 THEN 1 ELSE NULL END) AS 'WaitingNewsCount', 
                         COUNT(CASE WHEN News.newstatus = 7 THEN 1 ELSE NULL END) AS 'PublishedNewsCount', ByLine.ByLineName, ByLine.ByLineId
FROM            News INNER JOIN
                         News_Byline ON News.NewID = News_Byline.NewId INNER JOIN
                         ByLine ON News_Byline.ByLineId = ByLine.ByLineId
WHERE        (News.JournalID = 1) AND (News.AddedDate BETWEEN CONVERT(date, @DateFrom, 103) AND CONVERT(date, @DateTo, 103)) AND (News_Byline.ByLineId &lt;&gt; 336)
GROUP BY ByLine.ByLineName, ByLine.ByLineId</CommandText>
      </Query>
      <Fields>
        <Field Name="RejectedNewsCount">
          <DataField>RejectedNewsCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ByLineName">
          <DataField>ByLineName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="WaitingNewsCount">
          <DataField>WaitingNewsCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ByLineId">
          <DataField>ByLineId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PublishedNewsCount">
          <DataField>PublishedNewsCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
