@model IEnumerable<AkhbarElyoum.Models.NewsDetails>
@{bool flg = true;  int count = 0; string cssclass = ""; string JID = AkhbarElyoum.Controllers.HomeController.GetJournalID(HttpContext.Current.Request.Url.Host, int.Parse((HttpContext.Current.Request.QueryString["JournalID"] != null) ? HttpContext.Current.Request.QueryString["JournalID"].ToString() : "0"));}


<div class="col-md-8">



<!-- start Block1 With 5 News -->
@foreach (var item in Model.Where(a => a.DisplayOrder == 1))
{
    if (count == 0)
    {
        ViewContext.Writer.Write("<div class=\"news-section-2\">");
    }


    if (flg)
    {
    <div class="row">
        <div class="col-xs-12">
            <div class="section-header-normal">
                <a href="/news/newssection/@item.SectionID/@JID/@(AkhbarElyoum.Controllers.HomeController.GetPureURL(item.SecTitle)).html">
                    <span class="section-header-name">@item.SecTitle</span> <i class="fa fa-ellipsis-h fa-2x">
                    </i></a>
            </div>
        </div>
    </div>
        flg = false;
    }


    if (count < 1)
    {     
    <div class="row">
        <div class="col-xs-12">
            <div class="row">
                <a href="/news/newdetails/@item.NewsID/@JID/@(AkhbarElyoum.Controllers.HomeController.GetPureURL(item.Title)).html" class="news-section-2-block">
                    <div class="col-xs-5">
                        <img src="../images/images/large/@item.PictureName1" alt="@item.Title" class="img-responsive"/>
                    </div>
                    <div class="col-xs-7">
                        <span>@item.Title</span>
                        <p class="hidden-sm hidden-xs">
                            @item.Breif
                        </p>
                    </div>
                </a>
            </div>
        </div>
    </div>
    }

    count++;

    if (count == 1)
    {
        ViewContext.Writer.Write("<div class=\"row\">");
    }

    if (count > 1 && count <= 5)
    { 
    <div class="col-md-3 col-xs-6">
        <a href="/news/newdetails/@item.NewsID/@JID/@(AkhbarElyoum.Controllers.HomeController.GetPureURL(item.Title)).html" class="news-block-3">
            <img src="../images/images/small/@item.PictureName1" alt="@item.Title"  class="img-responsive"/>
            <span>@item.Title</span> </a>
    </div>
         
    }

    if (@item == Model.Where(a => a.DisplayOrder == 1).Last())
    {
        ViewContext.Writer.Write("</div></div>");
       
    }


}
<!-- End Block1 -->

