  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>NewsDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserID">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SectionID">
            <Value>=Parameters!SectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        AdminUsers.FullName, AdminUsers.UserID, COUNT(CASE WHEN News.Creator = AdminUsers.UserID THEN 1 ELSE NULL END) AS 'NewsPassCount', 
                         COUNT(CASE WHEN News.sUploaderID = AdminUsers.UserID THEN 1 ELSE NULL END) AS 'NewsPublishCount', MainSections.SecTitle
FROM            MainSections INNER JOIN
                         News ON MainSections.SectionID = News.SectionID LEFT OUTER JOIN
                         AdminUsers ON AdminUsers.UserID = News.sUploaderID OR AdminUsers.UserID = News.Creator
WHERE        (News.Approved = 1) AND (News.JournalID = 1) AND (News.PublishDate BETWEEN CONVERT(date, @DateFrom, 103) AND CONVERT(date, @DateTo, 103)) AND (News.NewStatus = 7) AND 
                         (AdminUsers.UserID = CASE WHEN @UserID = 0 THEN AdminUsers.UserID ELSE @UserID END) AND 
                         (MainSections.SectionID = CASE WHEN @SectionID = 0 THEN MainSections.SectionID ELSE @SectionID END)
GROUP BY AdminUsers.FullName, AdminUsers.UserID, MainSections.SecTitle</CommandText>
      </Query>
      <Fields>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NewsPassCount">
          <DataField>NewsPassCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UserID">
          <DataField>UserID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NewsPublishCount">
          <DataField>NewsPublishCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SecTitle">
